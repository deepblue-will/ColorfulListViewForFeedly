{
	"manifest_version": 2,

	"name": "Colorful List View for Feedly",
	"description": "Feedlyの一覧をFeedごとに色分けします。",
	"version": "1.0",
	"content_scripts": [
		{
			"matches": ["http://cloud.feedly.com/*"],
			"css": ["content.css"],
			"js": ["js/jquery.js", "js/content_script.js"],
			"run_at": "document_end"
		}
	],
	"icons": {
		"16": "img/icon16.png",
		"48": "img/icon48.png",
		"128": "img/icon128.png"
	}
}
