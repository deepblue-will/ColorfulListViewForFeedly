{
	"manifest_version": 2,

	"name": "Colorful List View for Feedly",
	"description": "Feedlyの一覧をFeedごとに色分けします。",
	"version": "0.2",
	"content_scripts": [
		{
			"matches": ["http://feedly.com/index.html*"],
			"css": ["content.css"],
			"js": ["js/jquery.js", "js/content_script.js"],
			"run_at": "document_end"
		}
	]
}
